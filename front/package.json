{
  "name": "front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve --host 0.0.0.0",
    "serve": "set NODE_OPTIONS=--openssl-legacy-provider && vue-cli-service serve --mode development",
    "build": "set NODE_OPTIONS=--openssl-legacy-provider && vue-cli-service build  --mode production",
    "cordova": "set NODE_OPTIONS=--openssl-legacy-provider && vue-cli-service build  --mode cordova && cd video_cordova && cordova run android",
    "lint": "set NODE_OPTIONS=--openssl-legacy-provider && vue-cli-service lint",
    "start": "set NODE_OPTIONS=--openssl-legacy-provider && webpack-dev-server --disableHostCheck=true --config ./web/webpack.config.dev.js"
  },
  "dependencies": {
    "@element-plus/icons": "0.0.11",
    "axios": "^0.21.1",
    "echarts": "^5.1.2",
    "element-plus": "^2.9.4",
    "gsap": "^3.11.5",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "vue": "^3.1.1",
    "vue-cropperjs": "^5.0.0",
    "vue-i18n": "^9.0.0",
    "vue-router": "^4.0.10",
    "vue-schart": "^2.0.0",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.1.2",
    "element-theme-chalk": "^2.15.13",
    "vue-template-compiler": "^2.6.11",
    "webpack-merge": "^5.2.0"
  }
}
