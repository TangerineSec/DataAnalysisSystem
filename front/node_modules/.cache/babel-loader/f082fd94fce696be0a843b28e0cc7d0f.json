         :drag=\"true\"\r\n              :limit=\"1\"\r\n              :auto-upload=\"false\">\r\n          </el-upload>\r\n        </el-form-item>\r\n\r\n      </el-form>\r\n      <template #footer>\r\n                <span class=\"dialog-footer\">\r\n                    <el-button @click=\"diaLogShow = false\">取 消</el-button>\r\n                    <el-button type=\"primary\" @click=\"submit\">确 定</el-button>\r\n                </span>\r\n      </template>\r\n    </el-dialog>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\n\r\nexport default {\r\n  name: \"table\",\r\n  data() {\r\n    return {\r\n      diaLogShow: false,\r\n      filename: '',\r\n      exportFileName: '',\r\n      showTableData: [],\r\n      fileList: [],\r\n      showTableHeader: [\r\n          { code: \"date\", name: \"赋分最低分\" },\r\n          { code: \"date\", name: \"80\" },\r\n          { code: \"date\", name:\"70\" },\r\n          { code: \"date\", name: \"65\" },\r\n          { code: \"date\",  name: \"60\" },\r\n          { code: \"date\", name: \"55\" },\r\n      ],\r\n    }\r\n  },\r\n  computed:{\r\n    showTableheader(){\r\n      const maxVisibleHeaders = 6; // 控制前10列\r\n      if (this.showTableHeader.length <= maxVisibleHeaders){\r\n        return this.showTableHeader;\r\n      } else {\r\n        return this.showTableHeader.slice(0,maxVisibleHeaders)\r\n      }\r\n    }\r\n  },\r\n  methods: {\r\n    handleProcess(){\r\n      // 处理excel,本来是加权，现在改为赋权，所以这里要改\r\n      const baseUrl = process.env.VUE_APP_BASE_URL\r\n      console.log(this.filename)\r\n      const url = baseUrl + '/excel_assign?filename=' + this.filename\r\n      axios.get(url).then(response => {\r\n        this.showTableHeader =  response.data.data.headers.slice(1)\r\n        this.showTableData =  response.data.data.data.slice(1)\r\n        this.filename =  ''\r\n        this.exportFileName =  response.data.data.filename\r\n      })\r\n\r\n    },\r\n    exportFile() {\r\n      // 本来是导出文件，现在也是导出文件，所以不改\r\n      const baseUrl = process.env.VUE_APP_BASE_URL\r\n      const url = baseUrl + '/excel_export?filename=' + this.exportFileName\r\n      window.open(url)\r\n    },\r\n    submit() {\r\n      // 本来是上传1个文件，现在也是上传一个文件，所以不改\r\n      const baseUrl = process.env.VUE_APP_BASE_URL\r\n      const url = baseUrl + '/upload_and_get_data_sheet'\r\n      let formData = new FormData();\r\n      const files = this.$refs.upload.uploadFiles\r\n      if (files.length === 0) {\r\n        alert('文件为空')\r\n        return\r\n      }\r\n      let file = files[0].raw\r\n      formData.append('file', file)\r\n      formData.append('pictureCategory','articleCover')\r\n      console.log(url);\r\n      // 添加请求头\r\n      axios.post(url,formData,{\r\n        'Content-type' : 'multipart/form-data'\r\n      })\r\n          .then(response => {\r\n            console.log(response.data)\r\n            if (response.data.code === 200) {\r\n              this.showTableHeader =  response.data.data.headers\r\n              this.showTableData =  response.data.data.data.slice(1)\r\n              this.filename =  response.data.data.filename\r\n              this.exportFileName = ''\r\n              this.$refs.upload.uploadFiles = []\r\n              this.diaLogShow = false\r\n            }\r\n          })\r\n\r\n    },\r\n\r\n    handleAdd() {\r\n      // 点击选择文件绑定的事件，导入之前操作基本不改\r\n      this.form = {}\r\n      this.diaLogShow = true\r\n      this.$refs.upload.uploadFiles = []\r\n    },\r\n  },\r\n  mounted() {\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n.container { /*page1_bg.png图片文件放置在Vue项目的静态资源目录中，引入背景*/\r\n  background-image: url(\"~@/assets/img/page7/bg.png\");\r\n  background-repeat: no-repeat;\r\n  background-size: cover;\r\n  background-attachment: fixed;\r\n}\r\n\r\n.handle-box {\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.handle-select {\r\n  width: 120px;\r\n}\r\n\r\n.handle-input {\r\n  width: 300px;\r\n  display: inline-block;\r\n}\r\n.table {\r\n  width: 100%;\r\n  font-size: 14px;\r\n  height: 720px;\r\n}\r\n.red {\r\n  color: #ff0000;\r\n}\r\n.mr10 {\r\n  margin-right: 10px;\r\n}\r\n.table-td-thumb {\r\n  display: block;\r\n  margin: auto;\r\n  width: 40px;\r\n  height: 40px;\r\n}\r\n</style>\r\n"],"mappings":";OAoBiCA,UAAiC;;EAfzDC,KAAK,EAAC;AAAW;;EACfA,KAAK,EAAC;AAAY;;EAClBC,IAAI,EAAC,sCAAsC;EAACC,MAAM,EAAC;;;EAsCxCF,KAAK,EAAC;AAAe;;;;;;;;;uBA5CzCG,mBAAA,CAmDM,c,4BAjDJC,mBAAA,CACM;IADDJ,KAAK,EAAC;EAAQ,4BAEnBI,mBAAA,CAmBM,OAnBNC,UAmBM,GAlBJD,mBAAA,CAMM,OANNE,UAMM,GALJF,mBAAA,CAA4I,KAA5IG,UAA4I,GAA7EC,YAAA,CAAyEC,oBAAA;IAA9DC,IAAI,EAAC,QAAQ;IAACC,IAAI,EAAC;;sBAAkB,MAAa,KAAAC,MAAA,QAAAA,MAAA,O,iBAAb,eAAa,mB;;QAC5HJ,YAAA,CAAuHC,oBAAA;IAA5GC,IAAI,EAAC,SAAS;IAACC,IAAI,EAAC,gBAAgB;IAAEE,OAAK,EAAEC,QAAA,CAAAC,SAAS;IAAEC,KAAgC,EAAhC;MAAA;IAAA;;sBAAiC,MAAO,KAAAJ,MAAA,QAAAA,MAAA,O,iBAAP,SAAO,mB;;kDAC3GJ,YAAA,CAAmHC,oBAAA;IAAxGC,IAAI,EAAC,SAAS;IAACC,IAAI,EAAC,gBAAgB;IAA2BE,OAAK,EAAEC,QAAA,CAAAG;;sBAAe,MAAO,KAAAL,MAAA,QAAAA,MAAA,O,iBAAP,SAAO,mB;;4CAA/CM,KAAA,CAAAC,QAAQ,S,mBAChEX,YAAA,CAAiHC,oBAAA;IAAtGC,IAAI,EAAC,SAAS;IAACC,IAAI,EAAC,gBAAgB;IAAiCE,OAAK,EAAEC,QAAA,CAAAM;;sBAAY,MAAE,KAAAR,MAAA,QAAAA,MAAA,O,iBAAF,IAAE,mB;;4CAA7CM,KAAA,CAAAG,cAAc,S,6BACtEjB,mBAAA,CAAuD,aAAlD,8CAA4C,oB,GAGnDI,YAAA,CAEWc,mBAAA;IAFD,YAAU,EAAC,SAAS;IAAEC,IAAI,EAAEL,KAAA,CAAAM,aAAa;IAAEC,MAAM,EAAN,EAAM;IAACzB,KAAK,EAAC,OAAO;IAAC0B,GAAG,EAAC,eAAe;IAAC,wBAAsB,EAAC;;sBAClG,MAAqC,E,kBAAtDvB,mBAAA,CAAwHwB,SAAA,QAAAC,WAAA,CAAlFV,KAAA,CAAAW,eAAe,GAA5BC,CAAC,EAAEC,KAAK;2BAAjCC,YAAA,CAAwHC,0BAAA;QAAhEC,IAAI,EAAEJ,CAAC,CAACK,IAAI;QAAGC,KAAK,EAAEN,CAAC,CAACO,IAAI;QAAGC,KAAK,EAAC;;;;yDAI/FlC,mBAAA,CAEM,cADJA,mBAAA,CAA2D;IAAtDY,KAAmB,EAAnB;MAAA;IAAA,CAAmB;IAACuB,GAAiC,EAAjCxC;4BAM7ByC,mBAAA,WAAc,EACdhC,YAAA,CAuBYiC,oBAAA;IAvBDC,KAAK,EAAC,iBAAiB;gBAAUxB,KAAA,CAAAyB,UAAU;+DAAVzB,KAAA,CAAAyB,UAAU,GAAAC,MAAA;IAAIC,KAAK,EAAC;;IAiBnDC,MAAM,EAAAC,QAAA,CACP,MAGO,CAHP3C,mBAAA,CAGO,QAHP4C,UAGO,GAFHxC,YAAA,CAAsDC,oBAAA;MAA1CI,OAAK,EAAAD,MAAA,QAAAA,MAAA,MAAAgC,MAAA,IAAE1B,KAAA,CAAAyB,UAAU;;wBA